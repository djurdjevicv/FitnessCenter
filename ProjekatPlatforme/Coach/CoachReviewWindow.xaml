<Window x:Class="ProjekatPlatforme.Coach.CoachReviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjekatPlatforme.Coach"
        mc:Ignorable="d"
        Title="CoachReviewWindow" Height="400" Width="700" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
                <RowDefinition Height="100"/>
            </Grid.RowDefinitions>
            <Label Grid.ColumnSpan="7" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="25" FontWeight="Bold">About our coach team</Label>
            <DataGrid AutoGenerateColumns="True" AutoGeneratedColumns="dataGrid_AutoGeneratedColumns" IsReadOnly="True" Name="dataGrid" Grid.Row="1" Grid.ColumnSpan="7" Grid.RowSpan="2" Margin="10,0" Width="680"></DataGrid>

            <Button Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="4" Click="ButtonSearch" FontSize="23" Margin="0,30" >Search coach</Button>
            <Button Grid.Column="4" Grid.ColumnSpan="2" Grid.Row="4" Click="ButtonBack" FontSize="23" Margin="0,30" >Back</Button>

        </Grid>
    </Grid>
</Window>
